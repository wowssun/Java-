CREATE TABLE t_memo
(mno NUMBER PRIMARY KEY,
mid VARCHAR2(50) REFERENCES t_member(MID),
memo VARCHAR2(100),
regdate DATE);

SELECT * FROM T_MEMO;

CREATE SEQUENCE Memo_seq
INCREMENT BY 1
START WITH 1
MAXVALUE 100
NOMINVALUE
NOCYCLE
NOCACHE;

SELECT COUNT (*) FROM T_MEMBER
WHERE MID = ? AND MPHONE=?;
